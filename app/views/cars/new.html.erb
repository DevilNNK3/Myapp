<%= form_for @car do |f| %>
 <label>Name</label>
  <%= f.text_field :name %><br />

  <label>Brand</label>
  <%= f.text_field :brand %><br />
  
  <label>Category</label>
  <%= f.text_field :category %><br />

  <label>Price</label>
  <%= f.text_field :price %><br />

  <%= f.submit "SAVE" %>
<% end %>